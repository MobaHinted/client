<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:vm="using:client.ViewModels"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" mc:Ignorable="d"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:i="https://github.com/projektanker/icons.avalonia"
             FontFamily="{StaticResource FiraMedium}" FontSize="16"
             RenderOptions.BitmapInterpolationMode="HighQuality"
             RenderOptions.EdgeMode="Antialias"

             d:DesignWidth="1765" d:DesignHeight="630"

             x:Class="client.Views.MatchHistoryView"
             x:DataType="vm:MatchHistory"
             Content="{Binding CurrentView}">
</UserControl>
