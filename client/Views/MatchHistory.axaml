<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:vm="using:client.ViewModels"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" mc:Ignorable="d"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             FontFamily="{StaticResource FiraMedium}" FontSize="16"
             RenderOptions.BitmapInterpolationMode="HighQuality"
             RenderOptions.EdgeMode="Antialias"

             d:DesignWidth="350" d:DesignHeight="550"

             x:Class="client.Views.MatchHistoryView"
             x:DataType="vm:MatchHistory">

    <!-- Content -->

    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"
                Orientation="Vertical" MinWidth="220">

        <TextBlock HorizontalAlignment="Center" FontSize="20" FontWeight="Bold"
                   Text="Match History" />
    </StackPanel>

    <!-- /Content -->
</UserControl>
